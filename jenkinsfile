
pipeline {
	agent any
	 tools {  
        	jdk '1.8.0_171' 
		maven 'manen-3.6.1'
    	}
	stages {
		stage('Initialize') {
			steps {
				sh '''
					echo "PATH = ${PATH}"
					echo "M2_HOME = ${M2_HOME}"
				'''
			}
		}
		stage('Build') {
			steps {
				echo "this is a minimal pipeline"
			}
		}
	}
}
